{
  "name": "vscode-graphiql-explorer",
  "version": "1.3.0",
  "engines": {
    "vscode": "^1.30.0"
  },
  "license": "MIT",
  "author": "Gabriel Nordeborn <gabbe.nord@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/zth/vscode-graphiql-explorer"
  },
  "publisher": "GabrielNordeborn",
  "activationEvents": [
    "workspaceContains:/schema.graphql",
    "workspaceContains:/schema.json",
    "workspaceContains:/graphql_schema.json",
    "workspaceContains:/.graphqlconfig",
    "onCommand:vscode-graphiql-explorer.edit",
    "onCommand:vscode-graphiql-explorer.show",
    "onCommand:vscode-graphiql-explorer.insert",
    "onCommand:vscode-graphiql-explorer.format"
  ],
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#171E26",
    "theme": "dark"
  },
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-graphiql-explorer.edit",
        "title": "Edit operation",
        "category": "GraphQL"
      },
      {
        "command": "vscode-graphiql-explorer.show",
        "title": "Explore schema with GraphiQL",
        "category": "GraphQL"
      },
      {
        "command": "vscode-graphiql-explorer.insert",
        "title": "Insert operation here",
        "category": "GraphQL"
      }
    ],
    "languages": [
      {
        "id": "graphql",
        "extensions": [
          ".gql",
          ".graphql",
          ".graphqls"
        ],
        "aliases": [
          "GraphQL"
        ],
        "configuration": "./graphql.configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "graphql",
        "scopeName": "source.graphql",
        "path": "./syntaxes/graphql.json"
      },
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.js.jsx",
          "source.tsx",
          "source.vue"
        ],
        "scopeName": "inline.graphql",
        "path": "./syntaxes/graphql.js.json",
        "embeddedLanguages": {
          "meta.embedded.block.graphql": "graphql"
        }
      },
      {
        "injectTo": [
          "source.reason"
        ],
        "scopeName": "inline.graphql.reason",
        "path": "./syntaxes/graphql.re.json",
        "embeddedLanguages": {
          "meta.embedded.block.graphql": "graphql"
        }
      }
    ]
  },
  "dependencies": {
    "graphiql": "^1.4.7",
    "graphiql-explorer": "^0.6.2",
    "graphql": "^14.5.8",
    "graphql-config": "2.2.2",
    "locate-character": "^2.0.5",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-error-boundary": "^1.2.5"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "yarn build",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "build": "yarn build:webview && yarn build:extension",
    "build:webview": "./scripts/build-non-split.js",
    "build:extension": "webpack -p",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.17",
    "@types/node-fetch": "^2.5.4",
    "@types/prettier": "^1.19.0",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/relay-config": "^6.0.0",
    "react-scripts": "^2.1.3",
    "rewire": "^4.0.1",
    "terser": "3.14.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "vscode": "^1.1.36",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
